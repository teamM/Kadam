<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>Kadam</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>LoginController</display-name>
    <servlet-name>LoginController</servlet-name>
    <servlet-class>com.kadam.controller.LoginController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginController</servlet-name>
    <url-pattern>/LoginController</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>DonorRegistrationController</display-name>
    <servlet-name>DonorRegistrationController</servlet-name>
    <servlet-class>com.kadam.controller.DonorRegistrationController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DonorRegistrationController</servlet-name>
    <url-pattern>/DonorRegistrationController</url-pattern>
  </servlet-mapping>
  <filter>
    <display-name>AuthenticationFilter</display-name>
    <filter-name>AuthenticationFilter</filter-name>
    <filter-class>com.kadam.filter.AuthenticationFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>AuthenticationFilter</filter-name>
    <url-pattern>/AuthenticationFilter</url-pattern>
  </filter-mapping>
  <servlet>
    <description></description>
    <display-name>ReceiptGenerationController</display-name>
    <servlet-name>ReceiptGenerationController</servlet-name>
    <servlet-class>com.kadam.controller.ReceiptGenerationController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ReceiptGenerationController</servlet-name>
    <url-pattern>/ReceiptGenerationController</url-pattern>
  </servlet-mapping>
  
</web-app>